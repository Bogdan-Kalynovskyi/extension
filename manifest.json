{
  "name": "My First Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/"
  ],
  "content_scripts": [
  {
    //"matches": ["http://*.livejournal.com/*"],
    "matches": ["http://localhost/*"],
    "run_at": "document_end",
    "js": ["jquery-1.9.0.js", "myscript.js"]
  }
  ]
}